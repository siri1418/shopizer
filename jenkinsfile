pipeline {
    agent { label 'u1' }
   triggers { pollSCM('* * * * *') }
    stages {
        stage ('vcs') {
            steps {
                 branch: 'develop',git url: 'https://github.com/siri1418/shopizer.git'
            }
        }
        stage ('build') {
        steps {
            sh 'mvn package'  
        }
    }
    stage ('test results formatted') {
        steps {
            junit '**/surefire-reports/*.xml'
        }
    
    }
    
}
